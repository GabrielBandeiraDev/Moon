{
    "version": 2,
    "builds": [
      {
        "src": "api/coins.js",
        "use": "@vercel/node"
      },
      {
        "src": "create-react-app/package.json" ,
        "use": "@vercel/static-build",
        "config": {
          "distDir": "create-react-app/build"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/coins.js" },
      { "src": "/(.*)", "dest": "/create-react-app/$1" }
    ]
  }
  